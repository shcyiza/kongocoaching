<%= simple_form_for(@kickstar) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :coaches_crew %>
    <%= f.association :crew %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
