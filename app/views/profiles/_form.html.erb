<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :coaches_crew %>
    <%= f.input :birth_date %>
    <%= f.input :sex %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
